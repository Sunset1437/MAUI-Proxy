<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MAUI_Proxy.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MAUI_Proxy"
    xmlns:views="clr-namespace:MAUI_Proxy.Views"
    Shell.FlyoutBehavior="Disabled"
    Title="MAUI_Proxy">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent
        Route="vpnpermission"
        ContentTemplate="{DataTemplate views:VpnPermissionPage}" />

</Shell>
